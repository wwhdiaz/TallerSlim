CREATE TABLE Customers (
	id int AUTO_INCREMENT NOT NULL,
	name VARCHAR(50) NOT NULL,
	PRIMARY KEY (id)
);

CREATE TABLE Gifts (
	id INT AUTO_INCREMENT NOT NULL,
	name VARCHAR(50) NOT NULL,
	price DECIMAL(8,2) NOT NULL,
	PRIMARY KEY (id)
);

CREATE TABLE Deliveries (
	id INT AUTO_INCREMENT NOT NULL,
	gift int NOT NULL,
	sender int NOT NULL,
	recipient int NOT NULL,
	message varchar(50),
	PRIMARY KEY (id),
	FOREIGN KEY (gift) REFERENCES Gifts(id),
	FOREIGN KEY (sender) REFERENCES Customers(id),
	FOREIGN KEY (recipient) REFERENCES Customers(id)
);

